 function MaxIncrements()
     return #CombatFrameWorkR.activeController.anims.basic
 end
 
 local cd = 0.1
 function Attack()
     spawn(function()
         while wait() do
             local Controller = Framework.Combat.activeController
             if Controller and tick() <= cd then
                 cd = tick() - 0.1
                 spawn(function()
                     Controller:attack()
                 end)
                 Controller.increment = MaxIncrements()
             end
         end
     end)
 end
 
 spawn(function()
     while wait() do
         if _G.FastAttack then
             pcall(function()
                 CameraShaker:Stop()
                 CombatFrameWorkR.activeController.attacking = false
                 if game.Players.LocalPlayer.Character:FindFirstChild("Electric Claw") or not game.Players.LocalPlayer.Character then 
                     CombatFrameWorkR.activeController.timeToNextAttack = -(math.huge ^ math.huge * math.huge)
                 elseif not game.Players.LocalPlayer.Character then
                     CombatFrameWorkR.activeController.timeToNextAttack = 2
                 else
                     CombatFrameWorkR.activeController.timeToNextAttack = -(math.huge * math.huge)
                 end
                 CombatFrameWorkR.activeController.increment = MaxIncrements()
                 CombatFrameWorkR.activeController.hitboxMagnitude = 80
                 CombatFrameWorkR.activeController.blocking = false
                 CombatFrameWorkR.activeController.timeToNextBlock = false
                 game.Players.LocalPlayer.Character.Stun.Value = 0
                 CombatFrameWorkR.activeController.hitSound = 0
                 RL.wrapAttackAnimationAsync = function(a,b,c,d,func)
                     local Hits = RL.getBladeHits(b,c,d)
                     if Hits then
                         PC.play = function() end
                         a:Play(0.01,0.01,0.01)
                         func(Hits)
                         PC.play = shared.cpc
                         wait(a.length * 0.5)
                         a:Stop()
                     end
                 end
             end)
